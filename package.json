{
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "dev": "gatsby develop",
    "lint-all-check": "npm-run-all -s --continue-on-error lint-tsc-check lint-eslint-check lint-prettier-check",
    "lint-all-fix": "npm-run-all -s lint-eslint-fix lint-prettier-fix",
    "lint-eslint-check": "eslint --ignore-path .gitignore 'src/**/*.{js,ts,tsx}'",
    "lint-eslint-fix": "eslint --fix --ignore-path .gitignore 'src/**/*.{js,ts,tsx}'",
    "lint-prettier-check": "prettier --check --ignore-path .gitignore src",
    "lint-prettier-fix": "prettier --write --ignore-path .gitignore src",
    "lint-tsc-check": "tsc",
    "serve": "gatsby serve",
    "test": "jest --no-cache"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "~1.6.5",
    "@popperjs/core": "^2.2.1",
    "@reach/router": "^1.3.3",
    "@types/classnames": "^2.2.10",
    "@types/d3": "^5.7.2",
    "@types/d3-array": "^2.0.0",
    "@types/lodash": "^4.14.149",
    "@types/ndarray": "^1.0.7",
    "@types/node": "^12.12.31",
    "@types/numeral": "^0.0.26",
    "@types/qs": "^6.9.1",
    "@types/reach__router": "^1.3.4",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/react-hot-loader": "^4.1.1",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-virtualized": "^9.21.8",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "chalk": "^3.0.0",
    "change-case": "^4.1.1",
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "d3": "^5.15.0",
    "d3-array": "^2.4.0",
    "date-fns": "^2.11.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "firebase": "^7.14.0",
    "flag": "^4.4.0",
    "gatsby": "^2.20.35",
    "gatsby-plugin-google-analytics": "^2.2.5",
    "gatsby-plugin-manifest": "^2.3.7",
    "gatsby-plugin-postcss": "^2.2.4",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.2.5",
    "gatsby-plugin-styled-components": "^3.2.4",
    "gatsby-plugin-typescript": "^2.3.5",
    "hex-alpha": "^1.0.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.9",
    "lodash": "^4.17.15",
    "map-obj": "^4.1.0",
    "ndarray": "^1.0.19",
    "npm-run-all": "^4.1.5",
    "numeral": "^2.0.6",
    "prettier": "^2.0.2",
    "qs": "^6.9.3",
    "react": "^16.0.0",
    "react-date-picker": "^8.0.1",
    "react-dom": "^16.0.0",
    "react-helmet": "^6.0.0",
    "react-hot-loader": "^4.12.20",
    "react-is": "^16.13.1",
    "react-popper": "^2.1.0",
    "react-spinners": "^0.8.1",
    "react-virtualized": "^9.21.2",
    "react-virtualized-auto-sizer": "^1.0.2",
    "regenerator-runtime": "^0.13.5",
    "resize-observer-polyfill": "^1.5.1",
    "semiotic": "^1.20.5",
    "styled-components": "^5.0.1",
    "tailwindcss": "^1.3.5",
    "typescript": "^3.8.3",
    "us-zcta-counties": "^0.0.2",
    "utility-types": "^3.10.0",
    "whatwg-fetch": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "!(*.d).ts": [
      "false -- Please use .tsx instead of .ts!"
    ],
    "*/**/*.js": [
      "false -- Please use .tsx with `// @ts-nocheck` instead of .js!"
    ],
    "*.{html,json,md,webmanifest}": [
      "prettier --write"
    ],
    "*.{d.ts,tsx}": [
      "bash -c tsc",
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true,
        "tsConfig": "<rootDir>/tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.tsx"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/database/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      ".+\\.(svg|css)$": "<rootDir>/src/__mocks__/svgMock.ts"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "@types/jest": "^25.2.1",
    "@types/react-tabs": "^2.3.1",
    "babel-jest": "^25.4.0",
    "jest": "^25.4.0",
    "jest-date-mock": "^1.0.8",
    "jest-styled-components": "^7.0.2",
    "ts-jest": "^25.4.0"
  }
}
